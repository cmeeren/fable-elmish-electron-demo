      "webpackConfig": "webpack.additions.renderer.js"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.10.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "4.0.0-alpha.53",
    "autosuggest-highlight": "^3.1",
    "match-sorter": "^4.1.0",
    "mdi-material-ui": "^6.15.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "source-map-support": "^0.5.19",
    "toastr": "^2",
    "typeface-roboto": "^0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "bufferutil": "^4.0",
    "devtron": "^1.4",
    "electron": "^9.0.0",
    "electron-builder": "^22.6.1",
    "electron-devtools-installer": "^3.0",
    "electron-webpack": "^2.8.2",
    "electron-window-state": "^5.0",
    "fable-compiler": "^2.9.0",
    "fable-loader": "^2.1.9",
